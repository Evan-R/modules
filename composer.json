{
  "name": "selene/framework",
  "description": "Selene php framework",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas Appel",
      "email": "mail@thomas-appel.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "symfony/console":"~2.5.0",
    "symfony/http-foundation":"~2.5.0",
    "symfony/http-kernel":"~2.5.0",
    "symfony/debug":"~2.5.0",
    "dsp/v6tools":"dev-master",
    "ircmaxell/password-compat":"1.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mockery/mockery": "0.9.*@dev",
    "mikey179/vfsStream": "1.2.0"
  },
  "replace": {
    "selene/cache": "self.version",
    "selene/common": "self.version",
    "selene/config": "self.version",
    "selene/console": "self.version",
    "selene/cryptography": "self.version",
    "selene/di": "self.version",
    "selene/events": "self.version",
    "selene/filesystem": "self.version",
    "selene/http": "self.version",
    "selene/kernel": "self.version",
    "selene/package": "self.version",
    "selene/routing": "self.version",
    "selene/stack": "self.version",
    "selene/testsuite": "self.version",
    "selene/view": "self.version",
    "selene/xml": "self.version"
  },
  "autoload": {
    "psr-4": {
      "Selene\\Components\\":""
    },
    "files": [
      "Common/Helper/functions.php"
    ]
  },
  "minimum-stability": "dev"
}
