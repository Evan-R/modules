{
  "name": "selene/framework",
  "description": "Selene php framework",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas Appel",
      "email": "mail@thomas-appel.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "symfony/http-foundation":"~2.4.0",
    "symfony/http-kernel":"~2.4.0",
    "symfony/console":"~2.4.0",
    "dsp/v6tools":"dev-master",
    "monolog/monolog":"dev-master",
    "ircmaxell/password-compat":"1.0.*",
    "twig/twig": "~1.12",
    "psr/log": "~1.0"
  },
  "require-dev": {
    "mockery/mockery": "dev-master",
    "mikey179/vfsStream": "dev-master",
    "phpspec/phpspec":"2.0.*@dev",
    "symfony/dependency-injection":"~2.4.0"
  },
  "config": {
    "bin-dir": "bin/"
  },
  "autoload": {
    "psr-4": {
      "Selene\\":"src/Selene/",
      "App\\":"app"
    },
    "files": [
      "src/Selene/Components/Common/Helper/functions.php"
    ]
  },
  "minimum-stability": "dev"
}
